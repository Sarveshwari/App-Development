<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".MainActivity">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="230dp"
        android:background="@drawable/background"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="0dip" />
    <LinearLayout
        android:id="@+id/LL1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:paddingBottom="0dp">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="40dp"
            app:srcCompat="@drawable/blackarrow" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="0dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="10dp"
            android:text="FEEDBACK"
            android:textColor="@color/black"
            android:fontFamily="@font/nats_regular"
            android:textSize="38sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/LL1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/nats_regular"
            android:textStyle="bold"
            android:layout_margin="0dp"
            android:paddingLeft="20dp"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:text="Rate your dietician" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/LL2"
        android:layout_marginLeft="40dp"
        android:orientation="horizontal"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/LL3"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nats_regular"
            android:layout_weight="1"
            android:textStyle="bold"
            android:layout_margin="0dp"
            android:paddingLeft="20dp"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:text="Rate your overall experience" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/LL4"
        android:layout_marginLeft="40dp"
        android:orientation="horizontal"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:padding="10dp"
            app:srcCompat="@drawable/star" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/LL5"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textStyle="bold"
            android:fontFamily="@font/nats_regular"
            android:layout_margin="0dp"
            android:paddingLeft="20dp"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:text="Give your feedback" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/LL6"

        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/RL2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="30dp">

            <EditText
                android:id="@+id/user_feedback"
                style="@style/account_edt_views1"
                android:layout_width="350dp"
                android:layout_height="170dp"
                android:layout_marginStart="7dp"
                android:layout_marginEnd="7dp"
                android:cursorVisible="false"
                android:elevation="4dp"
                android:focusableInTouchMode="true"
                android:gravity="top"

                android:hint="Type here ... "
                android:inputType="text"
                android:paddingHorizontal="15dp"
                android:paddingTop="5dp"
                android:textCursorDrawable="@drawable/custom_cursor"
                android:textSize="18sp" />

            <FrameLayout
                android:id="@+id/submitbtn"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:layout_below="@id/user_feedback"
                android:background="@drawable/rounded_background_button"
                android:layout_centerHorizontal="true">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/nats_regular"
                    android:gravity="center"
                    android:text="Submit"
                    android:textColor="#FFFFFF"
                    android:textSize="23sp" />

            </FrameLayout>

        </RelativeLayout>
    </LinearLayout>



</RelativeLayout>

